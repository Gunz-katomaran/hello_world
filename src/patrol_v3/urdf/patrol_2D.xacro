<?xml version="1.0"?>

<robot
  name="patrol_base"
  xmlns:xacro="http://www.ros.org/wiki/xacro"
  >

<xacro:include filename="$(find patrol_v3)/urdf/patrol_2D.gazebo" />
  <xacro:property name="cameraSize" value="0.05"/>
  <xacro:property name="cameraMass" value="0.1"/>
  <xacro:arg name="gpu" default="false"/>
  <xacro:property name="gpu" value="$(arg gpu)" />

  <link  name="base_link" />


  <link
    name="chassis">
    <inertial>
      <origin
        xyz="-0.05588 -0.021836 0.11979"
        rpy="0 0 0" />
      <mass
        value="60" />
      <inertia
        ixx="0.2844"
        ixy="0.00042222"
        ixz="0.045634"
        iyy="0.36594"
        iyz="-0.000122"
        izz="0.4528" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://patrol_v3/meshes/chassis.STL" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.79216 0.81961 0.93333 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://patrol_v3/meshes/chassis.STL" />
      </geometry>
    </collision>
  </link>

  <joint
    name="Jchassis"
    type="fixed">
    <origin
      xyz="0 0 0.2500"
      rpy="0 0 0" />
    <parent
      link="base_link" />
    <child
      link="chassis" />
    <axis
      xyz="0 0 0" />
  </joint>

  <link
    name="FL">
    <inertial>
      <origin
        xyz="-3.5376E-06 4.1624E-06 -0.041529"
        rpy="0 0 0" />
      <mass
        value="1.1763" />
      <inertia
        ixx="0.0072471"
        ixy="2.8453E-07"
        ixz="1.5822E-07"
        iyy="0.0072464"
        iyz="-6.4823E-07"
        izz="0.012903" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
