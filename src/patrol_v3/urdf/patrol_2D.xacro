      <inertia
        ixx="0.0072471"
        ixy="2.8396E-07"
        ixz="1.5827E-07"
        iyy="0.0072464"
        iyz="-6.4852E-07"
        izz="0.012903" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
