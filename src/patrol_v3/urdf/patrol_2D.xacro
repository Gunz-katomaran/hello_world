      </geometry>
      <material
        name="">
        <color
          rgba="0.29804 0.29804 0.29804 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://patrol_v3/meshes/RL.STL" />
      </geometry>
    </collision>
  </link>
  <joint
    name="RLJ"
    type="continuous">
    <origin
      xyz="-0.34867 0.23181 -0.1178"
      rpy="-1.5708 -0.47562 3.1416" />
    <parent
      link="chassis" />
    <child
      link="RL" />
    <axis
