    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://patrol_v3/meshes/FR.STL" />
      </geometry>
    </collision>
  </link>
  <joint
    name="FRJ"
    type="continuous">
    <origin
      xyz="0.29133 -0.27837 -0.1178"
      rpy="-1.5708 -0.32548 3.1416" />
    <parent
      link="chassis" />
    <child
      link="FR" />
    <axis
      xyz="0 0 -1" />
  </joint>
  <link
    name="RL">
    <inertial>
      <origin
        xyz="-3.5397E-06 4.7909E-06 -0.041529"
        rpy="0 0 0" />
      <mass
