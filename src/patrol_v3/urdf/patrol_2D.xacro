      rpy="1.5708 -1.5399 0" />
    <parent
      link="chassis" />
    <child
      link="FL" />
    <axis
      xyz="0 0 -1" />
  </joint>
  <link
    name="FR">
    <inertial>
      <origin
        xyz="-3.4705E-06 4.7858E-06 0.041533"
        rpy="0 0 0" />
      <mass
        value="1.1763" />
      <inertia
        ixx="0.0072471"
        ixy="2.854E-07"
        ixz="1.5817E-07"
        iyy="0.0072464"
        iyz="-6.4816E-07"
